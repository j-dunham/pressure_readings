<div class="row">
  <div> Summary for readings since <%= (Date.today - 5) %></div>
  <div class="md-6 d-flex justify-content-center">
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= @systolic_average %></h5>
        <h6 class="card-subtitle mb-2 text-muted">Average Systolic</h6>
      </div>
    </div>
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title"><%= @diastolic_average %></h5>
        <h6 class="card-subtitle mb-2 text-muted">Average Diastolic</h6>
      </div>
    </div>
  </div>
</div>